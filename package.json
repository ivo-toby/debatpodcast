{
    "name": "debatpodcast",
    "version": "1.0.0",
    "description": "Azure function to generate a RSS/Podcast feed from debat-direct data",
    "scripts": {
        "build": "./node_modules/typescript/bin/tsc",
        "watch": "tsc -w",
        "prestart": "npm run build",
        "start": "func start",
        "dev-generate-feed": "./node_modules/.bin/tsc-watch --onSuccess \"node index.js --function=GenerateFeed --mock=GenerateFeed \"",
        "test": "echo \"No tests yet...\"",
        "lint": "./node_modules/.bin/eslint . --ext .js,.jsx,.ts,.tsx ./",
        "fix": "./node_modules/.bin/eslint --fix --ext .js,.jsx,.ts,.tsx ./"
    },
    "dependencies": {
        "@hapi/joi": "^17.1.1",
        "axios": "^0.21.1",
        "azure-storage": "^2.10.3",
        "express-jwt": "^6.0.0",
        "joi-extract-type": "^15.0.8",
        "jsdom": "^16.4.0",
        "node-filter-async": "^2.0.0"
    },
    "devDependencies": {
        "@azure/functions": "^1.0.2-beta2",
        "@types/axios": "^0.14.0",
        "@types/node": "^14.6.3",
        "@typescript-eslint/eslint-plugin": "^3.10.1",
        "@typescript-eslint/parser": "^3.10.1",
        "azure-function-context-mock": "0.0.7",
        "eslint": "^7.8.1",
        "eslint-config-airbnb-typescript": "^9.0.0",
        "eslint-plugin-import": "^2.22.0",
        "eslint-plugin-jsx-a11y": "^6.3.1",
        "eslint-plugin-react": "^7.20.6",
        "eslint-plugin-react-hooks": "^4.1.0",
        "prettier": "^2.1.1",
        "tsc-watch": "^4.2.9",
        "typescript": "^3.9.7",
        "yargs": "^16.0.0-beta.1"
    }
}